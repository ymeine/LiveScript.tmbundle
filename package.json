{
	"name": "livescript",
	"version": "0.1.1",
	"publisher": "ymeine",
	"displayName": "LiveScript Language Support",
	"keywords": [
		"livescript",
		"LiveScript"
	],
	"engines": {
		"vscode": "*"
	},
	"categories": [
		"Languages"
	],
